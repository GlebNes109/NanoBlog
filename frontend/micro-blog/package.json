{
  "name": "micro-blog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage --coverage.threshold.lines=70 --coverage.threshold.functions=70 --coverage.threshold.branches=70 --coverage.threshold.statements=70",
